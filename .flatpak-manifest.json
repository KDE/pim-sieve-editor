{
    "id": "org.kde.sieveeditor",
    "branch": "master",
    "runtime": "org.kde.Platform",
    "runtime-version": "6.9",
    "base-version": "6.9",
    "base": "io.qt.qtwebengine.BaseApp",
    "sdk": "org.kde.Sdk",
    "command": "sieveeditor",
    "tags": [
        "nightly"
    ],
    "rename-icon": "sieveeditor",
    "desktop-file-name-suffix": " (Nightly)",
    "finish-args": [
        "--device=dri",
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--socket=pulseaudio",
        "--share=network",
        "--talk-name=org.kde.StatusNotifierWatcher",
        "--talk-name=org.kde.kwalletd6"
    ],
    "cleanup": [
        "*.a",
        "*.la",
        "/doc",
        "/docs",
        "/include",
        "/lib/cmake",
        "/lib/pkgconfig",
        "/lib/x86_64-linux-gnu/cmake",
        "/lib/x86_64-linux-gnu/pkgconfig",
        "/man",
        "/share/cmake",
        "/share/doc",
        "/share/man",
        "/share/pkgconfig"
    ],
    "modules": [
        {
            "name": "libsecret",
            "buildsystem": "meson",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.gnome.org/sources/libsecret/0.20/libsecret-0.20.5.tar.xz",
                    "sha256": "3fb3ce340fcd7db54d87c893e69bfc2b1f6e4d4b279065ffe66dac9f0fd12b4d",
                    "x-checker-data": {
                        "type": "gnome",
                        "name": "libsecret",
                        "stable-only": true
                    }
                }
            ],
            "config-opts": [
                "-Dgcrypt=false",
                "-Dgtk_doc=false",
                "-Dintrospection=false",
                "-Dmanpage=false",
                "-Dvapi=false"
            ]
        },
        {
            "name": "qtkeychain",
            "buildsystem": "cmake-ninja",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://github.com/frankosterfeld/qtkeychain/archive/0.14.2.tar.gz",
                    "sha256": "cf2e972b783ba66334a79a30f6b3a1ea794a1dc574d6c3bebae5ffd2f0399571",
                    "x-checker-data": {
                        "type": "anitya",
                        "project-id": 4138,
                        "stable-only": true,
                        "url-template": "https://github.com/frankosterfeld/qtkeychain/archive/$version.tar.gz"
                    }
                }
            ],
            "config-opts": [
                "-DCMAKE_INSTALL_LIBDIR=/app/lib",
                "-DLIB_INSTALL_DIR=/app/lib",
                "-DBUILD_TRANSLATIONS=NO",
                "-DBUILD_WITH_QT6=ON"
            ],
            "cleanup": [
                "/include",
                "/lib/cmake",
                "/mkspecs"
            ],
            "modules": [
                {
                    "name": "libsecret",
                    "buildsystem": "meson",
                    "config-opts": [
                        "-Dmanpage=false",
                        "-Dvapi=false",
                        "-Dgtk_doc=false",
                        "-Dintrospection=false",
                        "-Dgcrypt=false"
                    ],
                    "sources": [
                        {
                            "type": "archive",
                            "url": "https://download.gnome.org/sources/libsecret/0.20/libsecret-0.20.5.tar.xz",
                            "sha256": "3fb3ce340fcd7db54d87c893e69bfc2b1f6e4d4b279065ffe66dac9f0fd12b4d",
                            "x-checker-data": {
                                "type": "gnome",
                                "name": "libsecret",
                                "stable-only": true
                            }
                        }
                    ],
                    "cleanup": [
                        "/bin/secret-tool",
                        "/include",
                        "/lib/pkgconfig"
                    ]
                }
            ]
        },
